    {
      "source": "/ai-tools/:path*",
      "destination": "/docs/3wm-project/guides/:path*"
    }
  ],
  "primaryTab": {
    "name": "Documentatie"
  },
  "versions": [
    {
      "name": "v2.0",
      "url": "https://v2.docs.skydust.ai"
    },
    {
      "name": "v1.0",
      "url": "https://v1.docs.skydust.ai"
    }
  ],
  "theme": "quill",
  "modeToggle": {
    "default": "dark",
    "isHidden": false
  },
  "isWhiteLabeled": false,
  "robots": "index, follow",
  "lang": "nl-NL"
