    "prompt": "Zoek in 3WM documentatie..."
  },
  "metadata": {
    "og:title": "3WM AI Documentverwerking Documentatie",
    "og:description": "Enterprise factuurautomatisering aangedreven door AI",
    "og:image": "/images/hero-dark.png",
    "twitter:card": "summary_large_image",
    "twitter:site": "@skydustio",
    "twitter:title": "3WM Documentatie",
    "twitter:description": "Enterprise factuurautomatisering aangedreven door AI"
  },
  "analytics": {
    "ga4": {
      "measurementId": "G-XXXXXXXXXX"
    }
  },
  "seo": {
    "indexHiddenPages": false
  },
  "redirects": [
    {
      "source": "/essentials/:path*",
      "destination": "/docs/3wm-project/:path*"
